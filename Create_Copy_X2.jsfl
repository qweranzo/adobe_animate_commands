//// makeX2Copy//// This command was recorded by Adobe Animate.//// Layer Property: layerType, guidefl.getDocumentDOM().getTimeline().setLayerProperty('layerType', 'guide');// Lock Layers: truefl.getDocumentDOM().getTimeline().setLayerProperty('locked', true);// View Layers as Outlines: truefl.getDocumentDOM().getTimeline().setLayerProperty('outline', true);// Duplicate Layersfl.getDocumentDOM().getTimeline().duplicateLayers();// Layer Property: layerType, normalfl.getDocumentDOM().getTimeline().setLayerProperty('layerType', 'normal');// Lock Layers: falsefl.getDocumentDOM().getTimeline().setLayerProperty('locked', false);// View Layers as Outlines: falsefl.getDocumentDOM().getTimeline().setLayerProperty('outline', false);// Transform: 2, 0, 0, 2fl.getDocumentDOM().transformSelection(2, 0, 0, 2);// Convert to Bitmapfl.getDocumentDOM().convertSelectionToBitmap()// Transform: 0.500000, 0, 0, 0.500000fl.getDocumentDOM().transformSelection(0.500000, 0, 0, 0.500000);